<div 
  class="trpDisplayPicture  avatar-{{id}}  avatar-{{avatarSize}}  {{presenceClass}}  {{#if role}}{{role}}{{/if}}  {{#if inactive}}invited{{/if}}"
  style="background-image: url('{{avatarUrl}}')"
  data-original-title="{{#if showTooltip}}{{userDisplayName}}{{#if role}} ({{role}}){{/if}}{{/if}}">
  {{#showStatus}}<div class="trpStatus" id="status"> </div>{{/showStatus}}
  {{#showBadge}}<div class="trpDPUnreadCount">12</div>{{/showBadge}}
</div>
